@inject IConfiguration Configuration
@model AdBookingOrderListViewModel

@{
    ViewData["Title"] = "Orders";
    Layout = "~/Views/Shared/_Layout.cshtml";
    var version = Configuration.GetSection("Proshar")["Version"];
}
<div class="row">
    <div class="col-lg-12">
        @await Html.PartialAsync("_OrderSearchFormPartial", Model.SearchFilter)
        @await Html.PartialAsync("_OrderListPartial", Model)
    </div>

</div>

@section Scripts{    
    <script src="~/js/order/orderManager.js?v=@version"></script>
}

